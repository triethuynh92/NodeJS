<div class="row">
	<div class="col-lg-12">
		<h1 class="page-header">{{ title }}</h1>
	</div>
	<!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
	{{#each analyticCard}}
	<div class="col-lg-3 col-md-6">
		<div class="panel panel-{{ cardColor }}">
			<div class="panel-heading">
				<div class="row">
					<div class="col-xs-3">
						<i class="fa fa-{{ icon }} fa-5x"></i>
					</div>
					<div class="col-xs-9 text-right">
						<div class="huge">{{ totalCount }}</div>
						<div>{{ name }}</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	{{/each}}
</div>
